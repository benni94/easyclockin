{
  "name": "Easy Login",
  "description": "Helps to login easily.",
  "icons": {
    "16": "/icons/easyLogin16.png",
    "32": "/icons/easyLogin48.png",
    "48": "/icons/easyLogin48.png",
    "128": "/icons/easyLogin128.png"
  },
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"]
}
