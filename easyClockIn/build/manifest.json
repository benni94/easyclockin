{
  "name": "Easy clock in",
  "description": "Helps to clock in easily.",
  "icons": {
    "16": "/icons/easyClockIn16.png",
    "32": "/icons/easyClockIn32.png",
    "48": "/icons/easyClockIn48.png",
    "128": "/icons/easyClockIn128.png"
  },
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ]
}